exists("$$LH_DIR_SRCROOT/lcdhost.pro"):exists("$$LH_DIR_SRCROOT/lh_features/lcdhost.pri") {
    LH_VERSION=\$\$quote($$LH_VERSION)
    LH_DIR_SRCROOT=\$\$quote($$LH_DIR_SRCROOT)
    LH_DIR_DSTROOT=\$\$quote($$LH_DIR_DSTROOT)
